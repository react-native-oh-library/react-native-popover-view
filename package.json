{
  "name": "@react-native-oh-tpl/react-native-popover-view",
  "version": "5.1.7-0.0.1",
  "description": "A <Popover /> component for react-native iOS, Android, and Web",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "react-native": "src/index",
  "source": "src/index",
