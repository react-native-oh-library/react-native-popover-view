  "files": [
      "harmony",
      "lib",
      "src"
  ],
  "keywords": [
    "react-component",
    "react-native",
    "ios",
    "android",
    "ui",
    "popover",
    "modal",
    "harmony"
  ],
  "license": "MIT",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "tsc": "tsc",
    "start": "node_modules/react-native/packager/packager.sh",
    "test": "jest",
    "lint": "eslint src",
    "bob": "bob build",
